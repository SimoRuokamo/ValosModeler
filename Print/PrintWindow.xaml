<Window x:Class="ValosModeler.Print.PrintWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:ValosModeler.Print"
		mc:Ignorable="d"
		Title="Esikatselu" SizeToContent="Width" MinHeight="720" ResizeMode="CanResize">
	<ScrollViewer>
		<StackPanel x:Name="MainContent"  VerticalAlignment="Top" HorizontalAlignment="Left">
			<Button Content="Tulosta" Width="100" Margin="2" Command="{Binding PrintCommand}" />


		</StackPanel>
	</ScrollViewer>
</Window>
